{
  "name": "react-json-form-autofill",
  "version": "1.0.0",
  "description": "Auto-fill React forms using uploaded JSON files with field mapping, merge strategies, and validation.",
  "license": "MIT",
  "keywords": [
    "react",
    "json",
    "form",
    "autofill",
    "auto-fill",
    "form-filler",
    "upload",
    "hooks"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "sideEffects": false,
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/AshishSatyal/react-json-formfiller"
  },
  "homepage": "https://github.com/AshishSatyal/react-json-formfiller/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/AshishSatyal/react-json-formfiller/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "react": "^19.2.4",
    "tsup": "^7.2.0",
    "typescript": "^5.9.3"
  }
}
